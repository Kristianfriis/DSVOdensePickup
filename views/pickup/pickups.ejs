<%- contentFor('header') %> 
<%- include('../partials/navInside') %> 

<%- contentFor('body') %> 
<h2><i class="fa fa-car fa-lg"></i> Pickups</h2>
<h4>Adresses:</h4>
<table style="width: 100%;">
    <tr>
        <th>Name</th>
        <th>Address</th>
        <th>Postal Code</th>
        <th>City</th>
        <th>Mon</th>
        <th>Tue</th>
        <th>Wed</th>
        <th>Thu</th>
        <th>Fri</th>
        <th>Weight</th>
    </tr>
    <% data.map((data) => { %>
        <tr>
            <td><%= data.name %></td>
            <td><%= data.address %></td>
            <td><%= data.postalcode %></td>
            <td><%= data.city %></td>
            <td><%= data.mon %></td>
            <td><%= data.tue %></td>
            <td><%= data.wed %></td>
            <td><%= data.thu %></td>
            <td><%= data.fri %></td>
            <td><%= data.weight %></td>
        </tr>
    <% } )%> 
</table>
 